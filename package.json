{
  "name": "ol-comfy",
  "version": "1.0.0",
  "description": "Confortable and convenient ol shortcuts for standard usage.",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "_peerDependenciesComment_": "Dependencies with open range version, to grant liberties on final usage.",
  "peerDependencies": {
    "lodash": ">=4.17.21",
    "ol": ">=10.0.0",
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/lodash": "^4.17.16",
    "core-js": "^3.40.0",
    "eslint": "^8.33.0",
    "happy-dom": "^16.7.3",
    "prettier": "^3.5.2",
    "typedoc": "^0.28.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  },
  "scripts": {
    "build": "npm run check && npm run test && npm run build-only",
    "build-only": "tsc --skipLibCheck --pretty",
    "check": "npm run prettier && npm run lint",
    "doc": "typedoc --out apidoc --readme none --hideGenerator --entryPointStrategy Expand src",
    "lint": "eslint src/*.ts src/**/*.ts",
    "prettier": "npx prettier '{examples/,src/}{**/*,*}.{js,ts,jsx,tsx,html,css}' --write",
    "dev": "vite dev",
    "prod": "vite preview",
    "test": "vitest run",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ger-benjamin/ol-comfy.git"
  },
  "keywords": [
    "OpenLayers",
    "Utils"
  ],
  "author": "Camptocamp SA <info@camptocamp.com> (https://www.camptocamp.com)",
  "contributors": [
    "Benjamin Gerber <benjamin.gerber@camptocamp.com>"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ger-benjamin/ol-comfy/issues"
  },
  "homepage": "https://github.com/ger-benjamin/ol-comfy#readme"
}
